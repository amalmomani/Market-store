                    </div>
                    <table class="table table-dashboard mb-0 table-borderless fs--1 border-200">
                        <thead class="bg-light">
                            <tr class="text-900">
                                <th>picture</th>
                                <th>store</th>

                                <th class="pe-card text-end" style="width: 8rem">Sales</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var item in Model.Item5)
                            {
                            <tr>

                                <td>
                                    <img src=@Url.Content("~/Image/" + item.Image) style="width:70px; height:50px; object-fit:contain" />
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Shopname)
                                </td>
                                <td>
                                    @Html.DisplayFor(modelItem => item.Totalsales)

                                </td>
                                
                            </tr>
                            }
                        </tbody>
                    </table>
                </div>
